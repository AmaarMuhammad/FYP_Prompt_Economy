{
  "name": "prompt-economy",
  "version": "1.0.0",
  "description": "Decentralized Marketplace for AI Prompts - Iteration 1",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "client": "cd frontend && npm start",
